rt(true)\n    }\n\n    return (\n        <section className='success-purchase'>\n            <img className={classes.image} src={'Icons/s.png'}/>\n            <p className='success-advice'>Tu compra se ha completado!!</p>\n            <p className='label'>Tu código de seguimiento es:\n                <div className={classes.root}>\n                    <Toolbar >\n                        <div className={classes.search}>\n                            <InputBase\n                                 classes={{\n                                    root: classes.inputRoot,\n                                    input: classes.inputInput\n                                }}\n                                value={purchaseId}\n                                inputProps={{ \"aria-label\": \"search\", readOnly: true }}\n                            />\n                        </div>\n\n                        <div className={classes.search}>\n                            <CopyToClipboard text={value}\n                                             onCopy={handleCopy}>\n                                <FileCopyIcon />\n                            </CopyToClipboard>\n                        </div>\n                    </Toolbar>\n                </div>\n            </p>\n            <ActionAlert text='Copiado!' showAlert={showAlert} setShowAlert={setShowAlert}/>\n        </section>\n    )\n}\n\nexport default SuccessPurchase","/home/josuedebian/Proyectos/e-shop/src/components/WidgetCart/ItemCartWidget.jsx",["235"],"import React, {useContext} from \"react\";\nimport {Store} from \"../../Store\";\nimport './ItemCartWidget.css'\n\n\nconst ItemCartWidget =({article})=>{\n\n    const [data] = useContext(Store)\n\n    return(\n        <article key={article.id} className='item-widget'>\n            <div className='content-widget'>\n                <div className='descrip'>\n                    <div>\n                        {article.data.name}\n                    </div>\n                    <di